{
  "name": "backend",
  "version": "1.0.0",
  "description": "ParaConnect backend (Express + Mongo + Stripe)",
  "main": "index.js",
  "type": "commonjs",
  "engines": {
    "node": ">=18.17 <23"
  },
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "dev": "nodemon index.js",
    "dev:debug": "nodemon --inspect=0.0.0.0:9229 index.js",
    "seed:admin": "node scripts/seedAdmin.js",
    "backup:db": "node scripts/backup-db.js",
    "health": "node -e \"console.log('ok')\"",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:e2e": "node scripts/e2e-auth.js && node scripts/e2e-profile.js && node scripts/e2e-validation.js && node scripts/e2e-job-escrow.js && node scripts/e2e-matching.js && node scripts/e2e-messaging.js && node scripts/e2e-admin.js && node scripts/e2e-error-handling.js && node scripts/e2e-payouts.js",
    "test:e2e:auth": "node scripts/e2e-auth.js",
    "test:e2e:profile": "node scripts/e2e-profile.js",
    "test:e2e:validation": "node scripts/e2e-validation.js",
    "test:e2e:job-escrow": "node scripts/e2e-job-escrow.js",
    "test:e2e:matching": "node scripts/e2e-matching.js",
    "test:e2e:messaging": "node scripts/e2e-messaging.js",
    "test:e2e:admin": "node scripts/e2e-admin.js",
    "test:e2e:error-handling": "node scripts/e2e-error-handling.js",
    "test:e2e:payouts": "node scripts/e2e-payouts.js",
    "postinstall": "npm prune --production=false || true && npx puppeteer browsers install chrome || true"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.645.0",
    "@aws-sdk/lib-storage": "^3.940.0",
    "@aws-sdk/s3-request-presigner": "^3.645.0",
    "argon2": "^0.31.1",
    "archiver": "^7.0.1",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.6.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "nodemailer": "^6.9.15",
    "puppeteer": "^24.34.0",
    "stripe": "^18.4.0",
    "uuid": "^9.0.1",
    "web-push": "^3.6.2"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.0.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.0.0"
  },
  "overrides": {
    "follow-redirects": "^1.15.6"
  },
  "license": "UNLICENSED",
  "private": true
}
